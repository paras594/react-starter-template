{
  "name": "react-starter-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": "please-use-yarn",
    "node": "22",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "biome lint ./src",
    "format": "biome check --write ./src",
    "format:ci": "biome ci",
    "install:ci": "yarn install --frozen-lockfile",
    "install:prod": "yarn install --frozen-lockfile --non-interactive --production"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@tanstack/react-router": "^1.122.0",
    "@tanstack/react-router-devtools": "^1.122.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@tanstack/router-plugin": "^1.122.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.2.1",
    "daisyui": "^5.0.43",
    "globals": "^15.14.0",
    "typescript": "~5.7.2",
    "vite": "^6.0.11",
    "vite-plugin-pwa": "^0.21.1",
    "workbox-core": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "resolutions": {
    "sharp": "0.32.6",
    "sharp-ico": "0.1.5"
  }
}
